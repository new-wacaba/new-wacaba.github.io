          "_name": "features7",
          "_customHTML": "<section class=\"features7\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" value=\"7\" step=\"1\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" value=\"7\" step=\"1\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Item Count\" name=\"itemCount\" min=\"1\" max=\"6\" step=\"1\" value=\"4\">\n        <input type=\"checkbox\" title=\"Show Portrait\" name=\"showPortrait\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Item Text\" name=\"showItemText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Name\" name=\"showName\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Role\" name=\"showPosition\" checked=\"\">\n\n\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background9.jpg\" selected=\"\">\n            <input type=\"color\" title=\"Background Color\" value=\"#000000\">\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked=\"\" condition=\"bg.type!='color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n        <input type=\"range\" inline=\"\" title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"container\">\n        <div class=\"mbr-row main-row\">\n            <div class=\"text-block mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-12\">\n                <div class=\"text-wrap\">\n                    <h4 class=\"mbr-section-subtitle mbr-fonts-style align-center\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">\n                        Healthy Food\n                    </h4>\n                    <h3 class=\"mbr-section-title mbr-fonts-style mbr-white mbr-bold align-center\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title \">\n                        Our Vegan Store!\n                    </h3>\n                    <p class=\"mbr-fonts-style general-text mbr-text main-text mbr-white align-center\" mbr-theme-style=\"display-6\" mbr-if=\"showText\" data-app-selector=\".main-text\">\n                        Lorem ipsum dolor sit amet, consectetur adipisicing elit. Ut cupiditate doloribus ex vel quas nihil quod deserunt ipsum sit dignissimos!\n                    </p>\n                    <div mbr-list=\"\" mbr-list-grow=\"itemCount\" class=\"list-wrap d-flex mbr-justify-content-center\">\n                        <div class=\"list-item d-flex  mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6\">\n                            <div class=\"card-img  mbr-justify-content-center mbr-align-items-center\" mbr-if=\"showPortrait\">\n                                <div class=\"image-block\" mbr-if=\"showPortrait\">\n                                    <amp-img class=\"mobirise-loader\" src=\"@PROJECT_PATH@/assets/images/face5.jpg\" layout=\"responsive\" width=\"130\" height=\"130\" alt=\"Mobirise\" mbr-if=\"showPortrait\">\n                                        <div placeholder=\"\" class=\"placeholder\"></div>\n                                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/face5.jpg\" alt=\"Mobirise\">\n                                    </amp-img>\n                                </div> \n                            </div>\n                            <div class=\"content-wrapper d-flex mbr-justify-content-center\">\n                                <p class=\"item-text mbr-regular mbr-italic mbr-text mbr-white mbr-fonts-style \" mbr-theme-style=\"display-7\" mbr-if=\"showItemText\" data-app-selector=\".item-text\">\n                                    Lorem ipsum dolor sit amet, consectetur adipisicing elit. Ut cupiditate doloribus ex vel quas nihil quod deserunt ipsum sit dignissimos!\n                                </p>\n                                <div class=\"name_position d-flex mbr-align-items-center\">\n                                        <p class=\"item-title mbr-fonts-style mbr-bold mbr-white\" data-app-selector=\".item-title\" mbr-if=\"showName\" mbr-theme-style=\"display-6\">\n                                             Michael King\n                                        </p>\n                                    <span class=\" position item-position mbr-fonts-style\" mbr-text=\"\" data-app-selector=\".item-position\" mbr-if=\"showPosition\" mbr-theme-style=\"display-4\">\n                                        Manager\n                                    </span>\n                                </div>\n                            </div>\n                        </div>\n\n                        <div class=\"list-item d-flex  mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6\">\n                            <div class=\"card-img mbr-justify-content-center mbr-align-items-center\" mbr-if=\"showPortrait\">\n                                <div class=\"image-block\" mbr-if=\"showPortrait\">\n                                    <amp-img class=\"mobirise-loader\" src=\"@PROJECT_PATH@/assets/images/face6.jpg\" layout=\"responsive\" width=\"130\" height=\"130\" alt=\"Mobirise\" mbr-if=\"showPortrait\">\n                            